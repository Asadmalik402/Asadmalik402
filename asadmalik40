def hasil(ok,cp):
    if len(ok) != 0 or len(cp) != 0:
        print('\n\n\033[0mDone...')
        print('\n\033[1;92mOK : %s • CP : %s'%(str(len(ok)),str(len(cp))));exit()
        #print('\033[1;91mCHECK > %s'%(str(len(cp))));exit()
    else:
        print('\n\033[0mUps..No Results ')
        exit()
